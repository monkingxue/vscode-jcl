{
  "name": "vscode-jcl",
  "displayName": "vscode-jcl",
  "description": "",
  "version": "0.0.1",
  "publisher": "monkingxue",
  "engines": {
    "vscode": "^1.24.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Linters"
  ],
  "activationEvents": [
    "onCommand:extension.sayHello"
  ],
  "main": "./out/extension",
  "contributes": {
    "languages": [{
      "id": "JCL",
      "aliases": [
        "JCL",
        "jcl"
      ],
      "extensions": [
        ".jcl"
      ],
      "configuration": "./jcl.configuration.json"
    }],
    "commands": [{
      "command": "extension.sayHello",
      "title": "Hello World"
    }],
    "grammars": [{
      "language": "JCL",
      "scopeName": "source.jcl",
      "path": "./syntaxes/jcl.json"
    }],
    "snippets": [{
      "language": "JCL",
      "path": "./snippets/jcl.json"
    }]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.6",
    "tslint": "^5.8.0",
    "@types/node": "^7.0.43",
    "@types/mocha": "^2.2.42"
  }
}